	<block className="CapturingIndicator">
		<bind name="firstEntityDH" value="'avatar'; CC.playerAvatar"/>
		<bind name="watchDH" value="'isAlive'; ['avatar.health.evIsAliveChanged']; avatar.health.isAlive"/>
		<bind name="collectionDH" value="CC.interactiveZone; 'insideDropZones'; 'playerInside.dropZones'"/>
		<bind name="entityDH" value="'insideZone'; insideDropZones[0].id"/>
		<block>
			<bind name="name" value="'size'"/>
			<bind name="instance" value="'DropZonesIndicators'; isAlive &amp;&amp; !!(insideZone) ? {_zoneId: insideZone.id} : null"/>
		</block>
	</block>
	<block className="DropZonesIndicators">
		<bind name="entityDH" value="'insideZone'; _zoneId"/>
		<bind name="watchDH" value="'zoneIsActive'; ['insideZone.drop.evIsActiveChanged']; insideZone.drop.isActive"/>
		<bind name="watchDH" value="'zoneIsContested'; ['insideZone.drop.evIsContestedChanged']; insideZone.drop.isContested"/>
		<bind name="instance" value="'DropItemZoneLockIndicator'; zoneIsContested ? {} : null"/>
		<bind name="instance" value="'DropItemZoneInactiveIndicator'; !(zoneIsActive) ? {_zoneId: insideZone.id} : null"/>
	</block>
	<block className="DropItemZoneLockIndicator">
		<style>
			<align value="center"/>
			<flow value="horizontal"/>
		</style>
		<block>
			<style>
				<width value="27px"/>
				<height value="27px"/>
				<marginTop value="-7"/>
				<marginRight value="8px"/>
				<backgroundImage value="symbol:icon_personal_lock_enemy_ship"/>
			</style>
		</block>
		<block type="text">
			<styleClass value="$TextDefaultBold17NM"/>
			<bind name="text" value="'IDS_SHIP_MODIFIER_CAPTURING_INDICATOR'"/>
		</block>
	</block>
	<block className="DropItemZoneInactiveIndicator">
		<bind name="entityDH" value="'insideZone'; _zoneId"/>
		<bind name="watchDH" value="'timeoutTime'; ['insideZone.timeout.evTimeChanged']; insideZone.timeout.time"/>
		<bind name="watchDH" value="'timeoutIsRunning'; ['insideZone.timeout.evRunningChanged']; insideZone.timeout.running"/>
		<bind name="countdown" value="timeoutTime; 'countdownText'; 'mm:ss'"/>
		<bind name="visible" value="timeoutIsRunning"/>
		<style>
			<align value="center"/>
		</style>
		<block type="text">
			<styleClass value="$TextDefaultBold17NM"/>
			<bind name="text" value="'IDS_SHIP_MODIFIER_CAPTURING_INACTIVE'"/>
		</block>
		<block type="text">
			<style>
				<marginTop value="8px"/>
			</style>
			<styleClass value="$TextDefaultNM"/>
			<bind name="text" value="countdownText"/>
		</block>
	</block>
	<block className="RespawnTimerWrapper">
		<bind name="firstEntityDH" value="'avatar'; CC.playerAvatar"/>
		<bind name="watchDH" value="'isAlive'; ['avatar.health.evIsAliveChanged']; avatar.health.isAlive"/>
		<bind name="watchDH" value="'timeToRespawn'; ['avatar.playerAvatar.evTimeToRespawnChanged']; avatar.playerAvatar.timeToRespawn"/>
		<styleClass value="$FullsizeAbsolute"/>
		<block>
			<styleClass value="$MiddleVHAbsolutely"/>
			<bind name="instance" value="'RespawnTimer'; !(isAlive) &amp;&amp; !!(timeToRespawn) ? {} : null"/>
		</block>
	</block>
	<block className="RespawnTimer">
		<bind name="firstEntityDH" value="'avatar'; CC.playerAvatar"/>
		<bind name="watchDH" value="'willBeAliveAt'; ['avatar.playerAvatar.evTimeToRespawnChanged']; avatar.playerAvatar.willBeAliveAt"/>
		<bind name="watchDH" value="'timeToRespawn'; ['avatar.playerAvatar.evTimeToRespawnChanged']; avatar.playerAvatar.timeToRespawn"/>
		<bind name="countdown" value="willBeAliveAt; 'timeLeft'; 'mm:ss'; ''; ''; 'rawTimeLeft'"/>
		<bind name="dispatch" value="'addedToStage'; 'evDelayedShowCountdown'; {showCountdown: true}; 0; 1"/>
		<bind name="catch" value="'evDelayedShowCountdown'"/>
		<bind name="fade" value="showCountdown &amp;&amp; rawTimeLeft &amp;&amp; rawTimeLeft &gt; 0; 0.3; {alpha: 0}; {alpha: 1}"/>
		<block type="text">
			<styleClass value="$TextDefaultBold17NM"/>
			<style>
				<textAlign value="center"/>
				<width value="100%"/>
				<marginBottom value="8px"/>
				<leading value="-3"/>
			</style>
			<bind name="text" value="toUpperCase(tr('IDS_TIME_TILL_RESPAWN'))"/>
		</block>
		<block type="text">
			<styleClass value="$TextDefaultNM"/>
			<style>
				<textAlign value="center"/>
				<width value="100%"/>
				<marginBottom value="8px"/>
			</style>
			<bind name="text" value="timeLeft"/>
		</block>
		<block>
			<style>
				<width value="180px"/>
			</style>
			<block className="indicator_progress_bar_lightgreen" type="native">
				<style>
					<width value="100%"/>
				</style>
				<bind name="value" value="0"/>
				<bind name="maximum" value="1"/>
			</block>
			<block>
				<bind name="watch" value="'progressBarWidth'; math.round(100 - (rawTimeLeft / 1000 / timeToRespawn * 100))"/>
				<bind name="watch" value="'progressBarWidthClamped'; progressBarWidth &gt; 100 ? 100 : progressBarWidth"/>
				<style>
					<height value="2px"/>
					<marginTop value="1px"/>
					<backgroundColor value="0xFF4CE8AA"/>
					<position value="absolute"/>
				</style>
				<bind name="style" value="'width'; progressBarWidthClamped + '%'"/>
			</block>
		</block>
	</block>
