			<param name="name" value="FakeDock"/>
		</params>
		<style>
			<width value="100%"/>
			<height value="100%"/>
			<backgroundColor value="0x44FF00FF"/>
		</style>
		<bind name="controller" value="lesta.dialogs.dock.controllers.FakeCallbacksController"/>
		<block type="text">
			<style>
				<textColor value="0xffffffff"/>
			</style>
			<bind name="text" value="'Fake Dock'"/>
			<bind name="name" value="FakeDockText"/>
		</block>
		<block className="button_default" type="native">
			<params>
				<param name="name" value="FakeButtonCoop"/>
			</params>
			<style>
				<width value="300px"/>
			</style>
			<bind name="label" value="'SET BATTLE TYPE: COOPERATIVE'"/>
			<bind name="action" value="'click'; 'setBattleType'; {'battleType' : BattleTypes.COOPERATIVE_BATTLE }"/>
		</block>
		<block className="button_default" type="native">
			<params>
				<param name="name" value="FakeButtonTrainingReady"/>
			</params>
			<style>
				<width value="300px"/>
			</style>
			<bind name="label" value="'TRAINING ROOM: SET READY'"/>
			<bind name="action" value="'click'; 'setIsReadyInTrainingRoom'; {'isReady': true}"/>
		</block>
		<block className="button_default" type="native">
			<params>
				<param name="name" value="FakeButtonBattle"/>
			</params>
			<style>
				<width value="300px"/>
			</style>
			<bind name="label" value="'BATTLE'"/>
			<bind name="action" value="'click'; 'onBattleButtonClick'; {}"/>
		</block>
	</block>
	<block className="FakeRandomBattle">
		<params>
			<param name="name" value="FakeRandomBattle"/>
		</params>
		<style>
			<width value="100%"/>
			<height value="100%"/>
			<backgroundColor value="0x440000FF"/>
		</style>
		<block type="text">
			<style>
				<textColor value="0xffffffff"/>
			</style>
			<bind name="text" value="'Fake Random Battle'"/>
			<bind name="name" value="FakeRandomBattleText"/>
		</block>
	</block>
	<block className="FakeBattleLoading">
		<params>
			<param name="name" value="FakeBattleLoading"/>
		</params>
		<style>
			<width value="100%"/>
			<height value="100%"/>
			<backgroundColor value="0x4400FF00"/>
		</style>
		<block type="text">
			<style>
				<textColor value="0xffffffff"/>
			</style>
			<bind name="text" value="'Fake Battle Loading'"/>
			<bind name="name" value="FakeRandomBattleLoadingText"/>
		</block>
	</block>
</ui>
