			<flow value="htile"/>
		</style>
		<bind name="primaryEntityDH" value="'entity'; CC.commandMappingCommand; entityId"/>
		<bind name="watchDH" value="'list';	['entity.commandKeyList.evChanged'];	entity.commandKeyList.list"/>
		<block className="CMAdd">
			<bind name="controller" value="lesta.unbound.controls.UbInteractiveController"/>
			<bind name="transition" value="rollOver &amp;&amp; !mouseDown; 0.15; {alpha: 0.7}; {alpha: 1}"/>
