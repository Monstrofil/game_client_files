		<style>
			<flow value="horizontal"/>
		</style>
		<bind name="collectionRepeatDH" value="CC.commandMappingMod; 'CMKeyModItem'; ''; {mods: mods, device: device}"/>
	</block>
	<block className="CMKeyModItem">
		<bind name="dataRefDH" value="'entity'; '$dataRef.ref'"/>
		<bind name="watchDH" value="'used'; []; (entity.commandMappingMod.device == device) &amp;&amp; ((mods &amp; entity.commandMappingMod.index) &gt; 0)"/>
		<bind name="primaryEntityDH" value="'key'; CC.inputKey; entity.commandMappingModKey.keyCode"/>
		<bind name="visible" value="used"/>
		<block type="text">
			<styleClass value="$TextDefault"/>
			<bind name="substitute" value="''; 3; null; (key ? '[' + key.inputKey.name + ']' : 'IDS_EMPTY')"/>
