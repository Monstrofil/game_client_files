				<styleClass value="$TextDefaultBold"/>
				<style>
					<fontSize value="30"/>
					<marginTop value="-8px"/>
				</style>
				<text value="+"/>
			</block>
			<bind name="action" value="'click'; 'CommandMappingProxy.addKey'; {commandId: entity.commandMappingCommand.id, device: device}"/>
		</block>
		<bind name="repeat" value="list; 'CMKey'; {device : device}"/>
	</block>
	<block className="CMAdd">
		<bind name="controller" value="lesta.unbound.controls.UbInteractiveController"/>
		<bind name="transition" value="rollOver &amp;&amp; !mouseDown; 0.15; {alpha: 0.7}; {alpha: 1}"/>
		<style>
			<width value="32px"/>
			<height value="32px"/>
			<marginLeft value="5px"/>
			<backgroundColor value="0x60CCCCCC"/>
			<align value="center|middle"/>
		</style>
		<block type="text">
			<styleClass value="$TextDefaultBold"/>
			<style>
				<fontSize value="30"/>
				<marginTop value="-8px"/>
			</style>
			<text value="+"/>
		</block>
		<bind name="action" value="'click'; 'CommandMappingProxy.addKey'; {commandId: entity.commandMappingCommand.id, device: device}"/>
	</block>
	<block className="CMKey">
		<bind name="controller" value="lesta.unbound.controls.UbInteractiveController"/>
		<bind name="transition" value="rollOver &amp;&amp; !mouseDown; 0.15; {alpha: 0.7}; {alpha: 1}"/>
		<style>
			<marginLeft value="6px"/>
			<marginBottom value="6px"/>
			<height value="32px"/>
			<width value="45%"/>
			<backgroundSize value="fill"/>
			<align value="middle"/>
			<flow value="horizontal"/>
		</style>
		<bind name="var" value="{ keyTypes: {1: 'down', 2: 'up', 4: 'tap', 8: 'doubleTap', 16: 'forceTap', 32: 'longTap', 64: 'hold'} }"/>
		<bind name="entityDH" value="'entity'; $value"/>
		<bind name="firstEntityDH" value="'editEntity'; CC.commandMappingKeyEdit"/>
		<bind name="watchDH" value="'isEdit'; []; entity == editEntity"/>
		<bind name="watchDH" value="'isMyDevice'; ['entity.commandMappingKeyIndex.evChanged'];	entity.commandMappingKey.device == device"/>
		<bind name="watchDH" value="'keyId';	['entity.commandMappingKeyIndex.evChanged'];	entity.commandMappingKey.keyId"/>
		<bind name="watchDH" value="'mods';	['entity.commandMappingKeyIndex.evChanged'];	entity.commandMappingKey.mods"/>
		<bind name="primaryEntityDH" value="'key'; CC.inputKey; keyId"/>
		<bind name="style" value="'backgroundColor'; isEdit ? 0x20FF9933 : 0x20CCCCCC"/>
		<bind name="style" value="'backgroundSprite'; isEdit ? 'frame_warning' : 'frame_neutral'"/>
		<bind name="visible" value="((!!key &amp;&amp; isMyDevice) || isEdit)"/>
		<block>
			<style>
				<width value="100%"/>
				<height value="100%"/>
				<align value="center|middle"/>
				<backgroundColor value="0x01000000"/>
			</style>
			<block>
				<block className="CMKeyMods">
					<style>
						<flow value="horizontal"/>
					</style>
					<bind name="collectionRepeatDH" value="CC.commandMappingMod; 'CMKeyModItem'; ''; {mods: mods, device: device}"/>
				</block>
				<block type="text">
					<styleClass value="$TextHeaderWhiteBold"/>
					<style>
						<multiline value="false"/>
					</style>
					<bind name="substitute" value="''; 3; null; (key ? '[' + key.inputKey.name + ']' : 'IDS_EMPTY')"/>
				</block>
				<style>
					<flow value="horizontal"/>
				</style>
			</block>
			<block type="text">
				<styleClass value="$TextSecondary"/>
				<bind name="class" value="'$FontColorBlueish'"/>
				<style>
					<position value="absolute"/>
					<top value="-2px"/>
				</style>
				<bind name="text" value="toUpperCase(keyTypes[entity.commandMappingKey.keyType])"/>
